<% if post['type'] == 'post' %>
<article>
	<header>
		<h2><a href="/<%= post['key'] %>"><%= post['title'] %></a></h2>
		<% if post['published_at'] && published_at = Time.at(post['published_at'].to_i) %>
		<p>Posted on <time datetime="<%= published_at.strftime('%Y-%m-%dT%H:%M:%SZ') %>"><%= published_at.strftime('%B %e, %Y') %>
		</time></p>
		<% end %>
	</header>
	<%= post['html'] %>
</article>
<% else %>
<section class="page <%= post['key'] %>">
	<header>
		<h2><a href="/<%= post['key'] %>"><%= post['title'] %></a></h2>
	</header>
	<%= post['html'] %>
</section>
<% end %>